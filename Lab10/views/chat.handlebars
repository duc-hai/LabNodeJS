<div class="g_id_signout">Sign Out</div>
<div class="row justify-content-center">
  <div class="col-md-9">
    <div class="rounded border mt-3 mb-3">
      <div class="user">
        <div class="avatar">C</div>
        <div class="user-info">
          <div class="user-name">{{user.displayName}}</div>
          <div class="online">Vừa mới truy cập</div>
        </div>
        <div class="actions">
          <i class="fas fa-phone-alt"></i>
          <i class="fas fa-video"></i>
          <i class="fas fa-search"></i>
        </div>
      </div>
      <div id="messages" class="conversation">
      </div>
      <div class="message-input">
        <form>
          <div class="typing" hidden></div>
          <input type="text" placeholder="Nhập tin nhắn">
          <button type="submit" id="form-submit" class="btn btn-success btn-lg pull-right ">Gui</button>
        </form>
      </div>
    </div>

  </div>
</div>